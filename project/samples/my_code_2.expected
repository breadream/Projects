#include <iostream>
#include <stdio.h>
#include <string>
#include <math.h>
#include "include/Matrix.h"

using namespace std ;

int main () {
  bool flag;
  flag = true;
  int num;
  num = 1;
  int limit;
  limit = 10;
  int print_num;
  while (flag) {
    print_num = 3 * num;
    cout << print_num;
    cout << "\n";
    num = num + 1;
    if (num > limit) {
      flag = false;
    }
  }
}

